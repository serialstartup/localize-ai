{
  "expo": {
    "name": "HelpMeApp",
    "slug": "helpme-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9333ea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#9333ea"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "entryPoint": "./src/index.tsx",
    "plugins": [
      "expo-location"
    ]
  }
}
